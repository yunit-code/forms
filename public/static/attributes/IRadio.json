{
    "classId": "idm.componet.forms.iradio",
    "comName": "单选框",
    "className": "IRadio",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "",
            "helpUrl": ""
        },
        {
            "type": "group",
            "text": "基本属性",
            "desc": "提供设置组件能达到展示效果的基本属性设置信息",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "标题",
                    "bindKey": "label",
                    "default": "单选框"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "显示标题",
                    "bindKey": "labelDisplay",
                    "default": true
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "提示文字",
                    "bindKey": "placeholder",
                    "placeholder": "请输入"
                },
                {
                    "type": "textarea",
                    "layoutType": "inline",
                    "text": "默认值",
                    "bindKey": "defaultValue",
                    "default": ""
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "最大宽度",
                    "bindKey": "maxWidth",
                    "dictionary": [
                        {
                            "label": "auto",
                            "value": "auto"
                        },
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "rem",
                            "value": "rem"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ],
                    "default": 0
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "最小宽度",
                    "bindKey": "minWidth",
                    "dictionary": [
                        {
                            "label": "auto",
                            "value": "auto"
                        },
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "rem",
                            "value": "rem"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ],
                    "default": 0
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "右边距",
                    "bindKey": "rightPaddingNumber",
                    "dictionary": [
                        {
                            "label": "auto",
                            "value": "auto"
                        },
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "rem",
                            "value": "rem"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ],
                    "default": 0
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "标题排列",
                    "bindKey": "labelLayout",
                    "dictionary": [
                        {
                            "label": "横向排列",
                            "value": "horizontal"
                        },
                        {
                            "label": "纵向排列",
                            "value": "vertical"
                        }
                    ],
                    "default": "horizontal"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "尺寸",
                    "bindKey": "size",
                    "dictionary": [
                        {
                            "label": "小",
                            "value": "small"
                        },
                        {
                            "label": "中",
                            "value": "default"
                        },
                        {
                            "label": "大",
                            "value": "large"
                        }
                    ],
                    "default": "default"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "栅格布局",
                    "bindKey": "gridLayout",
                    "dictionary": [
                        {
                            "label": "无",
                            "value": "close"
                        },
                        {
                            "label": "6:12",
                            "value": "6:12"
                        },
                        {
                            "label": "6:18",
                            "value": "6:18"
                        },
                        {
                            "label": "12:12",
                            "value": "12:12"
                        }
                    ],
                    "default": "close",
                    "display": "@[labelLayout=='horizontal']"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "默认状态",
                    "bindKey": "defaultStatus",
                    "dictionary": [
                        {
                            "label": "普通",
                            "value": "default"
                        },
                        {
                            "label": "禁用",
                            "value": "disabled"
                        },
                        {
                            "label": "只读",
                            "value": "readonly"
                        },
                        {
                            "label": "隐藏",
                            "value": "hidden"
                        }
                    ],
                    "default": "default"
                },
                {
                    "type": "richText",
                    "layoutType": "inline",
                    "text": "描述内容",
                    "bindKey": "descContent"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "描述位置",
                    "bindKey": "descPosition",
                    "dictionary": [
                        {
                            "label": "横向",
                            "value": "horizontal"
                        },
                        {
                            "label": "纵向",
                            "value": "vertical"
                        }
                    ],
                    "default": "vertical",
                    "display": "@[gridLayout=='6:12']"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "排列方式",
                    "bindKey": "radioDirection",
                    "dictionary": [
                        {
                            "label": "横向",
                            "value": "horizontal"
                        },
                        {
                            "label": "纵向",
                            "value": "vertical"
                        }
                    ],
                    "default": "horizontal"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "单选模式",
                    "bindKey": "radioMode",
                    "dictionary": [
                        {
                            "label": "默认样式",
                            "value": "radio"
                        },
                        {
                            "label": "按钮样式",
                            "value": "button"
                        }
                    ],
                    "default": "radio"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "关闭校验",
                    "desc":"是否开启校验功能",
                    "bindKey": "closeCheck",
                    "default": false
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "校验占位",
                    "desc":"是否预留校验信息空间，开启将会在底部占用25px空白预留给校验信息使用",
                    "bindKey": "retainBottomHeight",
                    "display":"@[!closeCheck]",
                    "default": true
                }
            ]
        },
        {
            "type": "group",
            "text": "校验",
            "desc": "提供校验输入内容格式设置",
            "display":"@[!closeCheck]",
            "children": [
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "必填",
                    "bindKey": "required"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "必填错误提示",
                    "bindKey": "requiredText",
                    "default": "",
                    "display": "@[required]"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "自定义函数",
                    "bindKey": "customFun"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "函数代码",
                    "bindKey": "customFunCode",
                    "display": "@[customFun]",
                    "default": "function(value){return true}"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "自定义函数错误提示",
                    "bindKey": "customFunText",
                    "default": "",
                    "display": "@[customFun]"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "接口校验",
                    "bindKey": "interfaceVerify"
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "接口校验前置条件",
                    "bindKey": "interfaceVerifyPreconditionFunction",
                    "desc": "接口校验前会调用此函数判断是否需要进行校验，返回false则不继续下面的校验，为空则默认进行接口校验，接收参数{...自定义,urlData,pageId}",
                    "multiple": false
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "接口地址",
                    "bindKey": "interfaceVerifyUrl",
                    "display": "@[interfaceVerify]",
                    "desc":"接口返回如果为true代表验证通过，为false代码验证未通过，接收参数格式为{...自定义的,currentVal:当前输入结果}"
                },
                {
                    "type": "group",
                    "text": "校验未通过设置",
                    "desc": "提供接口校验未通过的后续设置",
                    "display": "@[interfaceVerify]",
                    "children": [
                        {
                            "type": "textarea",
                            "layoutType": "block",
                            "text": "未通过错误提示",
                            "bindKey": "interfaceVerifyText",
                            "default": ""
                        },
                        {
                            "type": "switch",
                            "layoutType": "inline",
                            "text": "显示弹窗",
                            "bindKey": "interfaceVerifyConfrim"
                        },
                        {
                            "type": "textarea",
                            "layoutType": "block",
                            "text": "确认窗显示内容",
                            "bindKey": "interfaceVerifyConfrimText",
                            "placeholder":"例如：该组件已存在，是否要跳转至管理界面？",
                            "default": ""
                        },
                        {
                            "type": "actionSelect",
                            "layoutType": "block",
                            "text": "弹窗确认自定义函数",
                            "bindKey": "interfaceVerifyConfrimOkAction",
                            "desc": "确认框显示点击确定后会调用此处设置的函数，接收参数格式为{...自定义的,interfaceResultData:校验接口返回的结果}"
                        }
                    ]
                }
            ]
        },
        {
            "type": "group",
            "text": "样式设置",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "标题文字",
                    "bindKey": "labelFont"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "错误提示文字",
                    "bindKey": "errorMsgFont"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "内容文字（编辑）",
                    "bindKey": "contentEditFont",
                    "desc": "编辑状态下的文字样式设置"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "内容文字（只读）",
                    "bindKey": "contentReadFont",
                    "desc": "只读情况下的文字样式设置"
                },
                {
                    "type": "group",
                    "text": "默认状态样式",
                    "desc": "单选未选中的状态样式",
                    "helpUrl": "",
                    "children": [
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "边框颜色",
                            "bindKey": "borderColorDefault"
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColor",
                            "placeholder": "请选择颜色",
                            "default": {}
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "选中状态样式",
                    "desc": "单选选中的状态样式设置",
                    "helpUrl": "",
                    "children": [
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "边框颜色",
                            "bindKey": "borderColorFocus"
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColorFocus",
                            "placeholder": "请选择颜色",
                            "default": {}
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "错误状态样式",
                    "desc": "单选校验未通过的错误情况下的样式设置",
                    "helpUrl": "",
                    "children": [
                        {
                            "type": "border",
                            "layoutType": "block",
                            "text": "边框设置",
                            "bindKey": "borderError"
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColorError",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "uploadImage",
                            "layoutType": "inline",
                            "text": "背景图片",
                            "bindKey": "bgImgUrlError",
                            "placeholder": "可输入图片地址或直接上传"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "横向偏移",
                            "bindKey": "positionXError",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "纵向偏移",
                            "bindKey": "positionYError",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "背景大小",
                            "bindKey": "bgSizeError",
                            "dictionary": [
                                {
                                    "label": "裁切显示",
                                    "value": "cover"
                                },
                                {
                                    "label": "完全显示",
                                    "value": "contain"
                                },
                                {
                                    "label": "自定义",
                                    "value": "custom"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "宽度",
                            "bindKey": "bgSizeWidthError",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSizeError=='custom']"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "高度",
                            "bindKey": "bgSizeHeightError",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSizeError=='custom']"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "平铺模式",
                            "bindKey": "bgRepeatError",
                            "dictionary": [
                                {
                                    "label": "双向重复",
                                    "value": "repeat"
                                },
                                {
                                    "label": "水平重复",
                                    "value": "repeat-x"
                                },
                                {
                                    "label": "垂直重复",
                                    "value": "repeat-y"
                                },
                                {
                                    "label": "不重复",
                                    "value": "no-repeat"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "inline",
                            "text": "背景模式",
                            "bindKey": "bgAttachmentError",
                            "dictionary": [
                                {
                                    "label": "固定",
                                    "value": "fixed"
                                },
                                {
                                    "label": "滚动",
                                    "value": "scroll"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "单项样式设置",
                    "desc": "单个单选选项样式设置",
                    "children":[
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "最大宽度",
                            "bindKey": "maxWidthItem",
                            "dictionary": [
                                {
                                    "label": "auto",
                                    "value": "auto"
                                },
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "rem",
                                    "value": "rem"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "default": 0
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "最小宽度",
                            "bindKey": "minWidthItem",
                            "dictionary": [
                                {
                                    "label": "auto",
                                    "value": "auto"
                                },
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "rem",
                                    "value": "rem"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "default": 0
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "右边距",
                            "bindKey": "rightMarginItem",
                            "dictionary": [
                                {
                                    "label": "auto",
                                    "value": "auto"
                                },
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "rem",
                                    "value": "rem"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "default": 0
                        },
                        {
                            "type": "box",
                            "layoutType": "block",
                            "text": "内外边距",
                            "bindKey": "boxItem"
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "背景设置",
                    "desc": "点击？查看背景设置用法指南",
                    "children":[
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColor",
                            "placeholder": "请选择颜色",
                            "default": {}
                        }
                    ]
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "box"
                }
            ]
        },
        {
            "type": "group",
            "text": "选项绑定",
            "desc": "点击？查看选项绑定设置用法指南",
            "children": [
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "数据来源",
                    "bindKey": "optionType",
                    "dictionary": [
                        {
                            "label": "静态数据",
                            "value": "static"
                        },
                        {
                            "label": "数据接口",
                            "value": "interface"
                        },
                        {
                            "label": "自定义函数",
                            "value": "customFun"
                        },
                        {
                            "label": "接收其他组件传值",
                            "value": "byval"
                        },
                        {
                            "label": "页面接口的结果集",
                            "value": "pageInterfaceResult"
                        }
                    ],
                    "default": "static",
                    "styleType": "radio"
                },
                {
                    "type": "table",
                    "text": "静态选项",
                    "bindKey": "optionStaticList",
                    "tabText": "选项[index]",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "显示值",
                            "bindKey": "text",
                            "placeholder": "请输入"
                        },
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "选项值",
                            "bindKey": "value",
                            "placeholder": "请输入"
                        },
                        {
                            "type": "select",
                            "layoutType": "inline",
                            "text": "值类型",
                            "bindKey": "valueType",
                            "desc": "设置“选项值”的类型，如果为数字，则会强制把“选项值”转换为number类型",
                            "dictionary": [
                                {
                                    "label": "数字",
                                    "value": "number"
                                },
                                {
                                    "label": "字符串",
                                    "value": "string"
                                }
                            ],
                            "default": "string"
                        },
                        {
                            "type": "switch",
                            "layoutType": "inline",
                            "text": "默认选中",
                            "bindKey": "check"
                        }
                    ],
                    "display": "@[optionType=='static']"
                },
                {
                    "type": "textarea",
                    "layoutType": "inline",
                    "text": "接口地址",
                    "bindKey": "optionInterfaceUrl",
                    "desc": "请按照统一标准的返回格式返回数组格式的数据，格式如下：{\"code\":\"200\",\"data\":[{\"text\":\"\",\"value\":\"\",\"check\":true}],...}",
                    "default": "",
                    "display": "@[optionType=='interface']"
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "自定义函数",
                    "bindKey": "optionFunction",
                    "desc": "获取选项数据源的时候会调用此方法，返回数据格式如下：[{\"text\":\"\",\"value\":\"\",\"check\":true}]",
                    "display": "@[optionType=='customFun']",
                    "multiple": false
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "值类型",
                    "bindKey": "byValType",
